<div class="col s2">
  <div class="row"></div>
  <div class="row">
    <div class="col s12"><label>抽出条件</label></div>
    <div class="col s12">
      <p>
        <input type="checkbox" id="condition-default" class="filled-in check-group-1" checked="checked" />
        <label for="condition-default">0.2未満</label>
      </p>
      <div class="divider"></div>
      <p>
        <input type="checkbox" id="condition-1" class="filled-in check-group-2" data-value-from="0.0" data-value-to="0.1" />
        <label for="condition-1">0.0 ～ 0.1</label>
      </p>
      <p>
        <input type="checkbox" id="condition-2" class="filled-in check-group-2" />
        <label for="condition-2">0.1 ～ 0.2</label>
      </p>
      <p>
        <input type="checkbox" id="condition-3" class="filled-in check-group-2" />
        <label for="condition-3">0.2 ～ 0.3</label>
      </p>
      <p>
        <input type="checkbox" id="condition-4" class="filled-in check-group-2" />
        <label for="condition-4">0.3 ～ 0.4</label>
      </p>
    </div>
    <div class="col s12">
      <button class="btn waves-effect waves-light" type="button" id="js-modify-filter">変更する</button>
    </div>
  </div>
</div>
<div class="col s10">
  <div class="row"></div>
  <div class="row">
    <div class="col s6" id="js-values-counter"></div>
    <div class="col s4 right">
      <div class="switch">
        <label class="right">
          すべて表示
          <input type="checkbox" id="js-switch-lever">
          <span class="lever"></span>
          条件に合致するもののみ表示
        </label>
      </div>
    </div>
  </div>
  <div class="row">
    <% @random_values.each do |rv| %>
      <div class="col s1">
        <div class="card">
          <div class="content-def">
            <%= rv %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
